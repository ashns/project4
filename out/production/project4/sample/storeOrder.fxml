<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="355.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.storeOrderController">
   <children>
      <ListView fx:id="ordersListView" layoutX="140.0" layoutY="14.0" prefHeight="373.0" prefWidth="200.0" />
      <Label layoutX="14.0" layoutY="14.0" text="Current Store Orders:" />
      <Button fx:id="removeBTN" onAction="#pressRemove" layoutX="14.0" layoutY="225.0" mnemonicParsing="false" prefHeight="72.0" prefWidth="113.0" text="Remove order" />
      <Button fx:id="returnBTN" layoutX="14.0" layoutY="311.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="72.0" prefWidth="113.0" text="Return to Main" />
      <Button fx:id="exportBTN" onAction="#pressExport" layoutX="13.0" layoutY="140.0" mnemonicParsing="false" prefHeight="72.0" prefWidth="113.0" text="Export Orders" />
   </children>
</AnchorPane>
