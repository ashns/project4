                        <ImageView fx:id="coffeeImageV" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@Image/tempCoffee.jpg" />
                           </image></ImageView>
                     </graphic>
                  </Button>
               </children>
            </VBox>
            <Label fx:id="tempL" layoutX="307.0" layoutY="42.0" text="Rutgers CafÃ©">
               <font>
                  <Font name="Bell MT" size="26.0" />
               </font></Label>
            <ImageView fitHeight="179.0" fitWidth="202.0" layoutX="307.0" layoutY="73.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Image/R%20logo.png" />
               </image>
            </ImageView>
